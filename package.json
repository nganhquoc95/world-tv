{
  "name": "worldtv",
  "version": "1.0.0",
  "description": "A TypeScript web app for browsing TV channels by country and category using the IPTV database",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run build:data && npm run build:client",
    "build:data": "node scripts/generate-json-data.js",
    "build:client": "npx vite build --outDir public/dist",
    "build:server": "tsc --project tsconfig.server.json",
    "server:dev": "nodemon --exec tsx src/index.ts --ext ts,tsx,json",
    "client:dev": "npx vite --host",
    "generate-channels": "ts-node scripts/parseChannels.ts",
    "test": "vitest"
  },
  "keywords": [
    "tv",
    "iptv",
    "channels",
    "database",
    "typescript",
    "express",
    "web"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^2.10.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "redux-saga": "^1.4.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/csvtojson": "^1.1.5",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-react": "^5.1.1",
    "csvtojson": "^2.0.14",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vite": "^7.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
